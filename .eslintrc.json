{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "2018",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "func-names": ["error", "never"],
        "no-console": "off"
    }
}
